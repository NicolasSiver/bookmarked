{
  "manifest_version": 3,
  "name": "Bookmarked",
  "version": "0.1.0",
  "description": "Manage bookmarks as a part of the new tab experience.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "unlimitedStorage"
  ]
}